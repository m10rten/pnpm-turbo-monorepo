{
  "name": "@monorepo/calculator",
  "scripts": {
    "build": "tsup src/**/*.ts"
  },
  "exports": {
    "./index": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.ts",
      "require": "./dist/index.ts"
    },
    "./math/add": {
      "types": "./dist/math/add.d.ts",
      "import": "./dist/math/add.ts",
      "require": "./dist/math/add.ts"
    },
    "./math/square": {
      "types": "./dist/math/square.d.ts",
      "import": "./dist/math/square.ts",
      "require": "./dist/math/square.ts"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "devDependencies": {
    "@monorepo/config": "workspace:*"
  }
}
